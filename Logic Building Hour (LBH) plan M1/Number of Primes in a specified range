#include<stdio.h>
#include<string.h>
#include<math.h>

// Helper function to check if a number is prime
int isPrime(int num) {
    if (num <= 1) return 0;
    if (num == 2) return 1;
    if (num % 2 == 0) return 0;
    
    for (int i = 3; i <= sqrt(num); i += 2) {
        if (num % i == 0) {
            return 0;
        }
    }
    return 1;
}

// Main function to count primes in range
int countPrimesInRange(int input1, int input2) {
    int count = 0;
    
    // Handle case where input1 is less than 2 (though assumption says >=2)
    if (input1 < 2) input1 = 2;
    
    for (int num = input1; num <= input2; num++) {
        if (isPrime(num)) {
            count++;
        }
    }
    return count;
}