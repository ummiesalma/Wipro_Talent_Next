import java.io.*;
import java.util.*;

// Read only region start
class UserMainCode {
    public int nFactorial(int input1) {  // Changed parameter name to input1
        // Read only region end
        // Write code here...
        int factorial = 1;
        for (int i = 1; i <= input1; i++) {  // Changed to use input1
            factorial *= i;
        }
        return factorial;
    }
}