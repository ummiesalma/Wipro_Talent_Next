#include<stdio.h>
#include<string.h>
#include<stdbool.h>

int uniqueDigitsCount(int input1) {
    bool seen[10] = {false}; // Track digits 0-9
    int count = 0;
    
    // Handle 0 case (though assumption says input >=1)
    if (input1 == 0) {
        return 1;
    }
    
    // Process each digit
    while (input1 != 0) {
        int digit = input1 % 10; // Get last digit
        if (!seen[digit]) {
            seen[digit] = true;
            count++;
        }
        input1 /= 10; // Remove last digit
    }
    
    return count;
}