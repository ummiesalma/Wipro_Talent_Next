import java.io.*;
import java.util.*;

// Read only region start
class UserMainCode {
    public int addLastDigits(int input1, int input2) {
        // Read only region end
        // Write code here...
        int last1 = Math.abs(input1 % 10);  // Get absolute value of last digit of first number
        int last2 = Math.abs(input2 % 10);  // Get absolute value of last digit of second number
        return last1 + last2;
    }
}
