#include<stdio.h>
#include<string.h>
#include<math.h>

// Function to check if a number is prime
int isPrime(int num) {
    if (num <= 1) return 0;
    if (num == 2) return 1;
    if (num % 2 == 0) return 0;
    
    for (int i = 3; i <= sqrt(num); i += 2) {
        if (num % i == 0) {
            return 0;
        }
    }
    return 1;
}

// Main function to find Nth prime
int NthPrime(int input) {
    if (input < 1) return -1;
    
    int count = 0;
    int num = 2;
    
    while (1) {
        if (isPrime(num)) {
            count++;
            if (count == input) {
                return num;
            }
        }
        num++;
    }
}